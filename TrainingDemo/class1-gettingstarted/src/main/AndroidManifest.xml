<?xml version="1.0" encoding="utf-8"?>
<manifest
    package="com.flyeek.gettingstarted"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name">
        <activity
            android:name="com.flyeek.gettingstarted.MainActivity"
            android:label="@string/main_activity_title"
            android:theme="@style/CustomActionBarTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".activitylifecircle.LifecircleDemoActivity"
            android:label="@string/lifecircle_demo_activity_title" />
        <activity
            android:name=".activitylifecircle.FullScreenActivity"
            android:label="@string/fullscreen_activity_title">
        </activity>
        <activity
            android:name=".activitylifecircle.DialogActivity"
            android:label="@string/dialog_activity_title"
            android:theme="@android:style/Theme.DeviceDefault.Dialog">
        </activity>
    </application>

</manifest>
